.mybut
	font 18px fuente
	color #fff	
	text-align center
	-webkit-border-radius 3px
	-moz-border-radius 3px
	border-radius 3px
	padding 11px 10px
	&.but-main
		background mainceleste		
		-webkit-box-shadow 0px 4px 0 mainceleste_hover
		-moz-box-shadow 0px 4px 0 mainceleste_hover
		box-shadow 0px 4px 0 mainceleste_hover
	&.but-main:hover
		background mainceleste_hover

	&.but-second
		background rosa
		padding 11px 10px
		margin 0
	&.but-continuar
		background amarillo
		-webkit-box-shadow 0px 4px 0 amarillo_hover
		-moz-box-shadow 0px 4px 0 amarillo_hover
		box-shadow 0px 4px 0 amarillo_hover
		width 100%
		padding 15px 25px
		text-align center
		color #fff
		font 22px fuente
		&:hover
			background amarillo_hover
