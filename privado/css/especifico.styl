#saludo
	max-width 430px
	position absolute
	bottom 20px
	left 0px
	background #fff
	padding 30px
	-webkit-box-shadow 0px 6px 0px rgba(0,0,0,0.2)
	-moz-box-shadow 0px 6px 0px rgba(0,0,0,0.2)
	box-shadow 0px 6px 0px rgba(0,0,0,0.2)
	border-radius 8px
	-webkit-transition left .7s ease-in-out, right .7s ease-in-out
	-moz-transition left .7s ease-in-out, right .7s ease-in-out
	-ms-transition left .7s ease-in-out, right .7s ease-in-out
	-o-transition left .7s ease-in-out, right .7s ease-in-out
	transition left .7s ease-in-out, right .7s ease-in-out
	em
		font  110px fuente_negrita
		color #03a6dc		
		text-transform uppercase
		margin 0 auto
		i:after
			content ','

	h1
		font  25px fuente
		color #858585
		padding 5px 0
		span
			font  25px fuente_negrita
			color #03a6dc
	p
		color #959595
		font 14px fuente_simple
		position relative
		padding-left 10px
		&:before
			content '*'
			color #ff576b
			position absolute
			left 0
			top 0

